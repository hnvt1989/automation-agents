{
  "functions": {
    "api/webhook.py": {
      "runtime": "python3.11",
      "maxDuration": 30,
      "memory": 512
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "api/webhook.py"
    },
    {
      "src": "/",
      "dest": "api/webhook.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}